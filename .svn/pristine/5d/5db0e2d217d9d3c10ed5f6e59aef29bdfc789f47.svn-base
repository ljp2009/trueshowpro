/* 模态框 */
.modal-mask {
width: 100%;
height: 100%;
position: fixed;
top: 0;
left: 0;
background: #000;
opacity: 0.5;
overflow: hidden;
z-index: 9000;
color: #fff;
}
.modal-dialog {
width: 600rpx;
overflow: hidden;
position: fixed;
top: 50%;
left: -20rpx;
z-index: 9999;
background: #f9f9f9;
margin: -180rpx 100rpx;
margin-right:200rpx;
/* border:1rpx solid red; */
border-radius: 36rpx;
}
