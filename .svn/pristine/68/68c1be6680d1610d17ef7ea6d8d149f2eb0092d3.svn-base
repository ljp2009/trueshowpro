/* pages/index/user/staff/index/index.wxss */
/* 头部 */
.topinfo{
  background-image: white
}
.text-center{
  font-size: 40rpx
}
/* 切换身份 */
.changeid{
  font-size: 25rpx
}
.emoji{
  font-size: 50rpx;
}
/* 技师基本信息 */
.message{
  background-color: white
}
.head-picture{
  /* border: 1px solid black */
  display: inline-block
}
.name{
  display: inline-block
}
.name-size{
  font-size: 45rpx
}
.info{
  display: inline-block
}
.experience,.thumbs-up,.receive{
  display: inline-block
}
.edit{
  margin-top: 90rpx;
  margin-left: 90rpx;
  font-size: 30rpx;
}
/* 目录部分 */
.cu-list.menu>.cu-item {
  position:relative;
  display:flex;
  padding:0 30rpx;
  min-height:100rpx;
  background-color:var(--white);
  justify-content:space-between;
  align-items:center;
  border-top:1rpx solid #eee;
}

.example {
  display: inline-block;
  width: 50%;
  height: 20px;
}

.marquee_box {
  width: 100%;
  position: relative;
}

.marquee_text {
  white-space: nowrap;
  position: absolute;
  top: 0;
}
