/* 弹幕样式*/
/**index.wxss**/
swiper{
  height: 100%;
}
.button{
  position: absolute;
  bottom: 0;
  width: 100%;
}
.aon{
position: absolute;
white-space:nowrap;/* 防止向下换行*/
animation-timing-function: linear;
animation-fill-mode: none;
}
.doommview{
 
  height: 20%;
  width: 100%;
  position: absolute;
  /* border: 3rpx solid green; */
}

@keyframes first{
  from{left: 100%; }
  to{left: -100%;}
}
.itemText{
 /* border: 1rpx solid red; */
 background-color: gray;
 opacity: 0.5;
 border-radius:15rpx ;
}
