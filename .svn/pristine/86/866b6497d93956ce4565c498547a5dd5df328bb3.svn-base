 <view class="padding bg-white">
    <view class="flex">
      <view class="flex-sub margin-xs">服务清单</view>
      <view class="flex-twice margin-xs "></view>
      <view class="flex-sub margin-xs"><text class="cuIcon-roundadd"></text></view>
    </view>
    <view class="flex">
      <view class="flex-sub  margin-xs secimg">
        <image src="https://ossweb-img.qq.com/images/lol/web201310/skin/big99008.jpg"/>
      </view>
      <view class="flex-twice  margin-xs serintro">
      <text>中医推拿60分钟</text>
