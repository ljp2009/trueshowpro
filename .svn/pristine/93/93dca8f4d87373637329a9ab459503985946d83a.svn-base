/* 约 */
.agreeContain{
  position: relative;

}
.agree{
  position: fixed;
  right: 0;
  top: 30%;
  height: 100rpx;
  width: 150rpx;
  z-index: 999;
  font-size: 20px; 
  background-color: rgb(255,102,0);
  border-radius: 50rpx 0 0 50rpx;
}
.agree .text{
  display: inline-block;
  border:1rpx solid red;
  margin-top:10rpx;
  margin-left:20rpx;
  padding-left: 15rpx;
  color:#fff;
  position: relative;
  right: 0;
  height: 80rpx;
  line-height:70rpx;
  width:80rpx;
  border-radius: 40rpx;
  background-color: gray;

}
.agree .count{
  position: absolute;
  color: white;
  font-size: 12px;
  background-color: red;
  width: 15px;
  height: 15px;
  line-height:15px;
  left: 70%;
  top: 0px;
  text-align: center;
  -webkit-border-radius: 15px;
  border-radius: 15px;
}
/*抛物线动画*/
.good_box{
  width: 18px;
  height: 18px;
  position: fixed;
  border-radius: 50%;
  overflow: hidden;
  left: 50%;
  top: 50%;
  z-index: +99;
  background:rgba(0,133,207,1);
}