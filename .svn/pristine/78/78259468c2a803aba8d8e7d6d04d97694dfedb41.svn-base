<!--pages/index/firm/joinStaff/join/join.wxml-->
<cu-custom bgColor="bg-gray" isBack="{{true}}" style="color:#000;">
  <view slot="backText" style='font-size:36rpx;color:#000;'>返回</view>
  <view slot="content" style='font-size:39rpx;margin-left:-50rpx;color:#000;'>加入机构</view>
</cu-custom>
<view class='bg-white' style='position:relative;height:400rpx;padding-top:45rpx;'>
  <view style='text-align:center;color:#333;font-size:36rpx;'>你是否确认作为成员加入：</view>
  <view style='height:20rpx;'></view>
  <view style='text-align:center;color:#333;font-weight:bold;font-size:42rpx;'>{{firmName}}</view>
  <view style='height:60rpx;'></view>
  <view style='text-align:center;color:#666;font-size:35rpx;'>（需等待机构负责人审核通过）</view>
  <view class="padding bg-white solid-bottom footer">
      <button class='radiu bg-white btnOne' bindtap='cancelBtn'>取消</button>
    
      <button class='radiu bg-orange btnTwo' bindtap='defineBtn'>确定</button>
  </view>
</view>