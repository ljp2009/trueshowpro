/* pages/admin/index/index.wxss */
/* 头部 */
.headline{
  margin: 30rpx auto 30rpx 45%;
  font-weight: bold;
  font-size: 24rpx;
  padding: 5rpx;
}
.headDown{
  margin-left: 20rpx;
  font-weight: bold;
  font-size: 20rpx;
}
/* 中心内容 */
.main{
  width: 100%;
  margin: auto auto 35rpx auto;
  font-size: 24rpx;
  float: left;
}
.main .left,.sum .left{
  display: inline-block;
  width: 33%;
  text-align: center;
  font-size: 24rpx;
  margin: auto;
}
.clear{
  clear: both;
}
.sum{
  border:1rpx solid gray;
  width: 96%;
  height: 10%;
  margin: 15rpx 2% 15rpx 2%;
  font-size: 24rpx;
  float: left;
}
.txtOne{
  margin-top: 25rpx;
  border-bottom: 1rpx solid #999;
  padding-left: 20rpx;
  padding-bottom: 20rpx;
  color: #000;
  font-size: 24rpx;
}
.txtTwo,.txtThree{
  padding-top: 25rpx;
  border-bottom: 1rpx solid #999;
  padding-left: 30rpx;
  padding-bottom: 20rpx;
  color:#000;
  font-size: 24rpx;
}
.txtTwo{
  padding-left: 35rpx;
  position: relative;
}
.txtThree{
  padding-left: 35rpx;
  position: relative;
}
.txtLeft{
  position: absolute;
  right: 60rpx;
}
.btn{
  position: absolute;
  right: 20rpx;
}