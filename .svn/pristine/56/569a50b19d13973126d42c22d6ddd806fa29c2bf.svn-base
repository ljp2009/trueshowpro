/* pages/index/firm/joinStaff/newFirm/newFirm.wxss */
.main{
  padding-left: 50rpx;
  margin: 15rpx auto;
  letter-spacing: 10rpx;
  font-size: 38rpx;
  color: #222;
}
input{
  margin: auto;
  border: 1rpx solid gray; 
  margin-bottom: 15rpx;
  padding-left: 15rpx;
}
/* 区域 */
.address{
  width: 90%;
  margin: auto;
}
.address input{
  float: left;
  font-size: 30rpx;
  padding-left: 15rpx;
  margin:1%;
}
/* 道路门牌号 */
.way{
  letter-spacing: normal;
}
/* 清除浮动 */
.clear{
  clear: both;
}
/* 标注 */
.label{
  width: 88%;
  margin: 50rpx auto;
  font-size: 30rpx;
  color: #222;
}
.btn{
  width: 50%;
  margin: auto;
  margin-top: 160rpx;
  letter-spacing: 20rpx;
  text-align: center;
  font-size: 40rpx;
  height: 88rpx;
  line-height: 88rpx;
}