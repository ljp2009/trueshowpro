/* pages/admin/certificate/certificate.wxss */
/* 头部 */
.headDown{
  text-align: center;
  font-weight: bold;
}
/* 中心内容 */
.main{
  font-weight: bold;
  margin-left: 30rpx;
}
/* 最外层容器 */
.wrap{
  position: relative;
  height: 100%;
}
/* 底部按钮容器 */
.footer{
  height: 6%;
  width: 100%;
  position: absolute;
  bottom: 0rpx;
  float: left
}
/* 不通过按钮 */
.btnOne{
  position: absolute;
  left: 7%;
  bottom: 25rpx;
  width: 42%;
  height: 70rpx;
  line-height: 70rpx;
  border: 1rpx solid #111;
}
/* 通过按钮 */
.btnTwo{
  position: absolute;
  right: 7%;
  width: 42%;
  height: 70rpx;
  bottom: 25rpx;
  line-height: 70rpx;
  letter-spacing: 20rpx;
}
